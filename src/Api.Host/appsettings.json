{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Json.JsonFormatter"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "RetryPolicy": {
    "RetriesCount": 3,
    "BreakDuration": "00:00:20",
    "RetryTimeout": "00:00:10",
    "ClientTimeout": "00:00:20"
  },
  "BankOfUk": {
    "Url": "https://localhost:7134"
  }
}
